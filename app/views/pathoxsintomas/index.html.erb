<h1>Listing pathoxsintomas</h1>

<table>
  <tr>
    <th>Itemlink</th>
    <th>Patologia</th>
    <th>Sintoma</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @pathoxsintomas.each do |pathoxsintoma| %>
  <tr>
    <td><%= pathoxsintoma.itemlink.id %></td>
    <td><%= pathoxsintoma.patologia.nome %></td>
    <td><%= pathoxsintoma.sintoma.nome %></td>
    <td><%= link_to 'Show', pathoxsintoma %></td>
    <td><%= link_to 'Edit', edit_pathoxsintoma_path(pathoxsintoma) %></td>
    <td><%= link_to 'Excluir', pathoxsintoma, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

